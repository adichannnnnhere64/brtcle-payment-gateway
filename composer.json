{
    "name": "adichan/payment",
    "description": "For Payment",
    "keywords": [
        "adichan",
        "transaction"
    ],
	"minimum-stability": "dev",
"prefer-stable": true,
    "homepage": "https://github.com/adichan/product",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Adrian Radores",
            "email": "mobistyle35@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "adichan/transaction": "dev-master",
        "adichan/wallet": "dev-master",
        "adichan/product": "dev-master",
        "illuminate/support": "^12.0",
        "paypal/paypal-checkout-sdk": "^1.0",
        "stripe/stripe-php": "^19.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.8.0",
	"driftingly/rector-laravel": "^2.1.3",
	"pestphp/pest": "^4.1.5",
        "fakerphp/faker": "^1.24.1",
        "laravel/tinker": ">=2.10.2",
        "larastan/larastan": "^3.8.0",
        "laravel/boost": "^1.8.3",
        "laravel/pail": "^1.2.4",
        "laravel/pint": "^1.26.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "pestphp/pest": "^4.1.5",
        "pestphp/pest-plugin-browser": "^4.1.1",
        "pestphp/pest-plugin-laravel": "^4.0.0",
        "pestphp/pest-plugin-type-coverage": "^4.0.3",
        "rector/rector": "^2.2.8"
    },
    "autoload": {
        "psr-4": {
            "Adichan\\Payment\\": "src",
            "Database\\Factories\\": "src/database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adichan\\Payment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adichan\\Payment\\PaymentServiceProvider"
            ],
	    "factories": "database/factories"
        }
    }
}
